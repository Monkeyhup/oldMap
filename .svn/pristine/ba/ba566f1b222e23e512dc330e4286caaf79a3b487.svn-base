package com.supermap.sgis.visual.json;

import java.util.List;
import java.util.Map;

/**
 * 综合excel数据导入时 参数接收类
 *
 * @author Created by W.Qiong on 14-9-3.
 */
public class MacroDataImport {
    /** 表id */
    private int matmid;

    /** 年份 */
    private int year;

    /** 月份 */
    private int month;

    /** 报告类型 */
    private int reportType;

    /** 行政区划级别 */
    private int regionLevel;

    /** 行政区划类型(地图)id */
    private int catalogId;

    /** 起始行号 */
    private int startRow;

    /** 起始列号 */
    private int startCol;

    /** 区划编码所在的列号 */
    private int regionCol;

    /** 分地区还是总量,默认地区 */
    private String excelFormat = "region";

    /** 行政区划（预留给总量） */
    private Region region;

    /** 指标列表 */
    private List<Map<String, String>> indicators;

    /** excel文件名 */
    private String fileName;

    /** excel工作簿名 */
    private String sheetName;

    /** 无参构造函数 */
    public MacroDataImport() {
    }

    /**
     * 取得excel工作簿名称
     *
     * @return excel工作簿名称
     */
    public String getSheetName() {
        return sheetName;
    }

    /**
     * 设置excel工作簿名称
     *
     * @param sheetName
     *            excel工作簿名称
     */
    public void setSheetName(String sheetName) {
        this.sheetName = sheetName;
    }

    /**
     * 取得excel文件名
     *
     * @return excel文件名
     */
    public String getFileName() {
        return fileName;
    }

    /**
     * 设置excel文件名
     *
     * @param fileName
     *            excel文件名
     */
    public void setFileName(String fileName) {
        this.fileName = fileName;
    }

    /**
     * 取得月份
     *
     * @return 月份
     */
    public int getMonth() {
        return month;
    }

    /**
     * 设置月份
     *
     * @param month
     *            月份
     */
    public void setMonth(int month) {
        this.month = month;
    }

    /**
     * 取得表id
     *
     * @return 表id
     */
    public int getMatmid() {
        return matmid;
    }

    /**
     * 设置表id
     *
     * @param matmid
     *            表id
     */
    public void setMatmid(int matmid) {
        this.matmid = matmid;
    }

    /**
     * 取得年份
     *
     * @return 年份
     */
    public int getYear() {
        return year;
    }

    /**
     * 设置年份
     *
     * @param year
     *            年份
     */
    public void setYear(int year) {
        this.year = year;
    }

    /**
     * 取得报告类型
     *
     * @return 报告类型
     */
    public int getReportType() {
        return reportType;
    }

    /**
     * 设置报告类型
     *
     * @param reportType
     *            报告类型
     */
    public void setReportType(int reportType) {
        this.reportType = reportType;
    }

    /**
     * 取得行政区划级别
     *
     * @return 行政区划级别
     */
    public int getRegionLevel() {
        return regionLevel;
    }

    /**
     * 设置行政区划级别
     *
     * @param regionLevel
     *            行政区划级别
     */
    public void setRegionLevel(int regionLevel) {
        this.regionLevel = regionLevel;
    }

    /**
     * 取得行政区划类型(地图)id
     *
     * @return 行政区划类型(地图)id
     */
    public int getCatalogId() {
        return catalogId;
    }

    /**
     * 设置行政区划类型(地图)id
     *
     * @param catalogId
     *            行政区划类型(地图)id
     */
    public void setCatalogId(int catalogId) {
        this.catalogId = catalogId;
    }

    /**
     * 取得起始行号
     *
     * @return 起始行号
     */
    public int getStartRow() {
        return startRow;
    }

    /**
     * 设置起始行号
     *
     * @param startRow
     *            起始行号
     */
    public void setStartRow(int startRow) {
        this.startRow = startRow;
    }

    /**
     * 取得起始列号
     *
     * @return 起始列号
     */
    public int getStartCol() {
        return startCol;
    }

    /**
     * 设置起始列号
     *
     * @param startCol
     *            起始列号
     */
    public void setStartCol(int startCol) {
        this.startCol = startCol;
    }

    /**
     * 取得区划编码所在的列号
     *
     * @return 区划编码所在的列号
     */
    public int getRegionCol() {
        return regionCol;
    }

    /**
     * 设置区划编码所在的列号
     *
     * @param regionCol
     *            区划编码所在的列号
     */
    public void setRegionCol(int regionCol) {
        this.regionCol = regionCol;
    }

    /**
     * 取得excel的格式化格式（分地区还是总量）
     *
     * @return excel的格式化格式（分地区还是总量）
     */
    public String getExcelFormat() {
        return excelFormat;
    }

    /**
     * 设置excel的格式化格式（分地区还是总量）
     *
     * @param excelFormat
     *            excel的格式化格式（分地区还是总量）
     */
    public void setExcelFormat(String excelFormat) {
        this.excelFormat = excelFormat;
    }

    /**
     * 取得行政区划（预留给总量）
     *
     * @return 行政区划（预留给总量）
     */
    public Region getRegion() {
        return region;
    }

    /**
     * 设置行政区划（预留给总量）
     *
     * @param region
     *            行政区划（预留给总量）
     */
    public void setRegion(Region region) {
        this.region = region;
    }

    /**
     * 取得指标列表
     *
     * @return 指标列表
     */
    public List<Map<String, String>> getIndicators() {
        return indicators;
    }

    /**
     * 设置指标列表
     *
     * @param indicators
     *            指标列表
     */
    public void setIndicators(List<Map<String, String>> indicators) {
        this.indicators = indicators;
    }
}