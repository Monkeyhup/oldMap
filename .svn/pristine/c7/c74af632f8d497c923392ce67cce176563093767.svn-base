package com.supermap.sgis.visual.json;

/**
 * 分段时间对象
 *
 * @author Created by zhangjunfeng on 14-4-22.
 */
public class TimeRank {

    /** 起始年份 */
    private int fromYear;

    /** 截止年份 */
    private int toYear;

    /** 起始月份 */
    private int fromMonth;

    /** 截止月份 */
    private int toMonth;

    /** 无参构造函数 */
    public TimeRank() {
    }

    /**
     * 构造函数，初始化数据
     *
     * @param fromYear
     *            起始年份
     * @param toYear
     *            截止年份
     * @param fromMonth
     *            起始月份
     * @param toMonth
     *            截止月份
     */
    public TimeRank(int fromYear, int toYear, int fromMonth, int toMonth) {
        this.fromYear = fromYear;
        this.toYear = toYear;
        this.fromMonth = fromMonth;
        this.toMonth = toMonth;
    }

    /**
     * 取得 起始年份
     *
     * @return 起始年份
     */
    public int getFromYear() {
        return fromYear;
    }

    /**
     * 设置 起始年份
     *
     * @param fromYear
     *            起始年份
     */
    public void setFromYear(int fromYear) {
        this.fromYear = fromYear;
    }

    /**
     * 取得截止年份
     *
     * @return 截止年份
     */
    public int getToYear() {
        return toYear;
    }

    /**
     * 设置截止年份
     *
     * @param toYear
     *            截止年份
     */
    public void setToYear(int toYear) {
        this.toYear = toYear;
    }

    /**
     * 取得 起始月份
     *
     * @return 起始月份
     */
    public int getFromMonth() {
        return fromMonth;
    }

    /**
     * 设置 起始月份
     *
     * @param fromMonth
     *            起始月份
     */
    public void setFromMonth(int fromMonth) {
        this.fromMonth = fromMonth;
    }

    /**
     * 取得 截止月份
     *
     * @return 截止月份
     */
    public int getToMonth() {
        return toMonth;
    }

    /**
     * 设置 截止月份
     *
     * @param toMonth
     *            截止月份
     */
    public void setToMonth(int toMonth) {
        this.toMonth = toMonth;
    }
}
