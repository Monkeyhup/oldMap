package com.supermap.sgis.visual.json;

/**
 * 存储字段信息
 * <p>
 * &nbsp;&nbsp;说明：<br/>
 * &nbsp;&nbsp;存储key,value的形式
 * </p>
 *
 * @author Created by zhangjunfeng on 14-4-23.
 */
public class FieldInfo {
    /** 键（key） */
    private String key;

    /** 键值 */
    private String value;

    /**
     * 构造函数，初始化数据
     *
     * @param key
     *            键（key）
     * @param value
     *            键值
     */
    public FieldInfo(String key, String value) {
        this.key = key;
        this.value = value;
    }

    /**
     * 取得key
     *
     * @return key
     */
    public String getKey() {
        return key;
    }

    /**
     * 设置key
     *
     * @param key
     *            key名
     */
    public void setKey(String key) {
        this.key = key;
    }

    /**
     * 取得value值
     *
     * @return value值
     */
    public String getValue() {
        return value;
    }

    /**
     * 设置value值
     *
     * @param value
     *            value值
     */
    public void setValue(String value) {
        this.value = value;
    }
}
